    "dev": "cross-env-shell VITE_BUILD_PLATFORM=web \"vite\"",
    "build": "echo 'WARNING: Use yarn build:web to build for web instead, or use yarn build:app to build for app instead. Building for web...' && yarn build:web",
    "build:base": "tsc && vite build",
    "build:app": "cross-env-shell VITE_BUILD_PLATFORM=app \"yarn build:base && rm -rf dist/js/*.map && cap copy\"",
    "build:web": "cross-env-shell VITE_BUILD_PLATFORM=web \"yarn build:base && echo 'Artifacts built under dist/'\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "deploy:ios": "cap sync ios",
    "gen:supabase": "supabase gen types typescript --project-id lbtpnmdffuimikksrvns > src/models/supabase.types.ts"
